model Appointment {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  modules     String?
  date        DateTime
  uuid        String?
  office      Office   @relation(fields: [officeId], references: [id])
  officeId    Int
}

model Office {
  id           Int           @id @default(autoincrement())
  name         String
  subsidiary   String
  appointments Appointment[]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
 
generator client {
  provider = "prisma-client-js"
}